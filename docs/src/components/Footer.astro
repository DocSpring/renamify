---
import Default from '@astrojs/starlight/components/Footer.astro';
---

<Default {...Astro.props}>
  <slot />
</Default>
<div class="docspring-footer" style="margin-top: 2rem; padding-top: 2rem; border-top: 1px solid var(--sl-color-gray-5);">
  <div style="text-align: center; padding: 1rem 0;">
    <strong>Renamify was created by <a href="https://docspring.com" style="color: var(--sl-color-text-accent);">DocSpring</a></strong>
    <br />
    <span style="color: var(--sl-color-gray-3); font-size: 0.875rem;">
      Fill, sign, and generate PDFs at scale
    </span>
  </div>
</div>
